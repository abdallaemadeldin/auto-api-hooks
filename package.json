{
  "name": "auto-api-hooks",
  "version": "1.0.4",
  "description": "Generate React Query hooks, SWR hooks, and TypeScript types from OpenAPI 3.x, Swagger 2.0, and GraphQL specs. API client code generator with Zod schema validation and MSW v2 mock server generation.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdallaemadeldin/auto-api-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/abdallaemadeldin/auto-api-hooks/issues"
  },
  "homepage": "https://github.com/abdallaemadeldin/auto-api-hooks#readme",
  "author": "Abdalla Emad Eldin",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "auto-api-hooks": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "chokidar": "^4.0.0",
    "commander": "^12.1.0",
    "graphql": "^16.9.0",
    "picocolors": "^1.1.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "react-hooks",
    "openapi",
    "swagger",
    "graphql",
    "codegen",
    "code-generation",
    "react-query",
    "tanstack-query",
    "swr",
    "axios",
    "fetch",
    "typescript",
    "zod",
    "msw",
    "mock-server",
    "api-client",
    "hooks-generator"
  ]
}